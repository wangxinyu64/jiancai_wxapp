/* component/nav-text/nav-text.wxss */

.topBar {
  width: 750rpx;
  /* position: fixed; */
  top: 0;
  left: 0;
  z-index: 9999;
  background: #fff;
  color: #000;
}

.font-1 {
  font-size: 40rpx;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  color: rgba(0, 0, 0, 1);
}

.font-0 {
  font-size: 36rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(17, 17, 17, 1);
}

.statusBar {
  width: 750rpx;
  /* background: linear-gradient(90deg,#FF0001,#FE6030); */
}

.navigationBar {
  color: black;
  text-align: center;
  font-size: 36rpx;
  width: 750rpx;
  position: relative;
}

.page-name {
  float: left;
  margin-left: 20rpx;
  /* font-size: 40rpx; */
  font-family: PingFangSC-Regular;
  font-weight: bold;
  color: #000;
}

.navbar-title {
  float: right;
  /* display: inline-block; */
  position: relative;
}

.setting {
  display: inline-block;
  width: 49rpx;
  height: 46rpx;
  vertical-align: middle;
}

.setting image {
  width: 100%;
  height: 100%;
  vertical-align: top;
}

.navBack {
  position: absolute;
  top: 10rpx;
  height: 80rpx;
  width: 24rpx;
  left: 30rpx;
}

.return {
  width: 24rpx;
  height: 40rpx;
}
.back{
  float: left;
  margin-left: 20rpx;
  width: 43rpx;
  height: 100%;
}
.back image{
  width: 23rpx;
  height: 40rpx;
  vertical-align: middle;
}